import java.util.*;
   class Add{
    int a,b;
         Scanner sc=new Scanner(System.in);
    void operands(){
          System.out.println("Enter the first no.");
   a=sc.nextInt();
        System.out.println("Enter the second no.");
  b=sc.nextInt();
   }
   void plus(){
       System.out.println("Sum of a and b is =" +(a+b));
  }
  }
    class Addition{
          public static void main(String[]args){
          Add a=new Add();
         a.operands();
      a.plus();
    }
}
